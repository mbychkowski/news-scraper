<nav>
  <div class="nav-wrapper blue-grey darken-1">
    <a href="#" class="brand-logo right">NPR tiny desk scrapper</a>
  </div>
</nav>

<div class="container">
  <div class="row">

    {{#each concerts}}

      <div class="col s12 m12">
        <div class="card horizontal">
          <div class="card-stacked">
            <div class="card-content">
              <h5 class="header">{{this.artist}}</h2>
              <p>{{this.teaser}}</p>
            </div>
            <div class="card-action">
              <a href="{{this.videoUrl}}">watch</a>
              <a href="#">Comment</a>
            </div>
          </div>
          <div class="card-image">
            <img src="{{this.imgUrl}}">
          </div>
        </div>
      </div>


    {{/each}}

  </div>
</div>
